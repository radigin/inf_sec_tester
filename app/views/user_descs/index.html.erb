<p id="notice"><%= notice %></p>

<h1>Listing User Descs</h1>

<table>
  <thead>
    <tr>
      <th>Lname</th>
      <th>Fname</th>
      <th>Sname</th>
      <th>Address</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_descs.each do |user_desc| %>
      <tr>
        <td><%= user_desc.lname %></td>
        <td><%= user_desc.fname %></td>
        <td><%= user_desc.sname %></td>
        <td><%= user_desc.address %></td>
        <td><%= user_desc.user %></td>
        <td><%= link_to 'Show', user_desc %></td>
        <td><%= link_to 'Edit', edit_user_desc_path(user_desc) %></td>
        <td><%= link_to 'Destroy', user_desc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User desc', new_user_desc_path %>
